@model HMS.Models.RecipteModel
@{
    Layout = null;
}
<head>
    <link rel="stylesheet" type="text/css" href="http://yui.yahooapis.com/2.7.0/build/reset-fonts-grids/reset-fonts-grids.css" media="all" />
    <link rel="stylesheet" href="~/Content/resume.css">
</head>
<body>
    <div id="doc2" class="yui-t7">
        <div id="inner">
            <div id="hd">
                <div class="yui-gc">
                    <div class="yui-u first">
                        <h2>Hospital Management System</h2>
                        <br>
                        <h3>Дата: @Convert.ToDateTime(Session["Start_date_recipe"]).ToString("dd-MM-yyyy") 
                        <br/>Крайна дата: @Convert.ToDateTime(Session["End_date_recipe"]).ToString("dd-MM-yyyy")</h3>
                    </div>
                    <div class="yui-u">
                        <div class="contact-info">
                        </div><!--// .contact-info -->
                    </div>
                </div><!--// .yui-gc -->
            </div><!--// hd -->
            <div id="bd">
                <div id="yui-main">
                    <div class="yui-b">
                        <div class="yui-gf">
                            <div class="yui-u first">
                                <h2>Провел прегледа</h2>
                            </div>
                            <div class="yui-u">
                                <p class="enlarge">
                                    Име: д-р.@Session["D_first_name"] @Session["D_family_name"]
                                </p>
                                <p class="enlarge">
                                    Телефон: @Session["D_phone"]
                                </p>
                                <p class="enlarge">
                                    Град: @Session["doc_city"]
                                </p>
                            </div>
                        </div><!--// .yui-gf -->
                        <div class="yui-gf">
                            <div class="yui-u first">
                                <h2>Пациент</h2>
                            </div>
                            <div class="yui-u">
                                <p class="enlarge">
                                    Име: @Session["P_first_name"] @Session["P_family_name"]
                                </p>

                                <p class="enlarge">
                                    Пол: @Session["P_gender"]
                                </p>
                                <p class="enlarge">
                                    Телефон: @Session["D_phone"]
                                </p>
  
                            </div>
                        </div><!--// .yui-gf -->
                        <div class="yui-gf">
                            <div class="yui-u first">
                                <h2>Тип рецепта: </h2>
                            </div>
                            <div class="yui-u">
                                <div class="talent">

                                    <div class="talent">
                                        <h2>@Session["firstDd"] </h2>
                                    </div>
                                </div>
                            </div><!--// .yui-gf -->
                            <div class="yui-gf last">
                                <div class="yui-u first">
                                    <h2></h2>
                                </div>
                                <div class="yui-u">
                                    <h2></h2>
                                </div>
                            </div><!--// .yui-gf -->

                            <div class="yui-gf">
                                <div class="yui-u first">
                                    <br />
                                    <h2>Вид рецепта: </h2>
                                </div><!--// .yui-u -->
                                <div class="yui-u">

                                    <div class="job">
                                        <h2> @Session["secondDd"]</h2>

                                        <br />
                                    </div>

                                </div><!--// .yui-u -->
                            </div><!--// .yui-gf -->

                            <div class="yui-gf last">
                                <div class="yui-u first">
                                    <h2>Под вид: </h2>
                                </div>
                                <div class="yui-u">
                                    <h2>@Session["ssecondDd"]</h2>
                                </div>
                            </div><!--// .yui-gf -->
                            <div class="yui-gf last">
                                <div class="yui-u first">
                                    <h2>Тема: </h2>
                                </div>
                                <div class="yui-u">
                                    <h2>@Session["name_recipe"]</h2>
                                </div>
                            </div><!--// .yui-gf -->
                            <div class="yui-gf last">
                                <div class="yui-u first">
                                    <h2>Лекарства: </h2>
                                </div>
                                <div class="yui-u">
                                    <h2>@Session["description_recipe"]</h2>
                                </div>
                            </div><!--// .yui-gf -->
                            <div class="yui-gf last">
                                <div class="yui-u first">
                                    <h2>Номер на протокола: </h2>
                                </div>
                                <div class="yui-u">
                                    <h2>@Session["number_protocol"]</h2>
                                </div>
                            </div><!--// .yui-gf -->

                        </div><!--// .yui-b -->
                    </div><!--// yui-main -->
                </div><!--// bd -->

            </div><!-- // inner -->

        </div>
</body>
